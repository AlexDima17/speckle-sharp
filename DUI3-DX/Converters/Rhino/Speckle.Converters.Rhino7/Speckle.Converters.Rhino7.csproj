<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net48</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
    </PropertyGroup>

  <PropertyGroup Condition="'$(IsDesktopBuild)' == false">
    <WarningsNotAsErrors>
      $(WarningsNotAsErrors);
      CS8618;
    </WarningsNotAsErrors>
  </PropertyGroup>

  <ItemGroup>
    <!-- Include must be the fully qualified .NET type name of the Attribute to create. -->
    <AssemblyAttribute Include="Speckle.Converters.Common.SpeckleConversionAssembly">
    </AssemblyAttribute>
  </ItemGroup>
  
  <ItemGroup>
    <PackageReference Include="RhinoCommon" Version="7.13.21348.13001" IncludeAssets="compile;build"/>
  </ItemGroup>
    <ItemGroup>
      <ProjectReference Include="..\..\..\Sdk\Speckle.Autofac\Speckle.Autofac.csproj"/>
      <ProjectReference Include="..\..\..\Sdk\Speckle.Converters.Common\Speckle.Converters.Common.csproj"/>
    </ItemGroup>

</Project>
